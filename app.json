{
  "expo": {
    "name": "ruapp",
    "slug": "ruapp",
    "scheme": "ufsm-ru",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#181818"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#181818"
      },
      "package": "dev.nirewen.ruapp"
    },
    "plugins": [["./plugins/withDrawableAssets.plugin.js", "./assets/shortcuts"], "expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0e7f9525-1eb1-4d14-b04f-9be0369d4251"
      }
    }
  }
}
